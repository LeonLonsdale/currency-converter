{
  "bracetSpacing": true,
  "bracketSameLine": false,
  "singleQuote": true,
  "jsxSingleQuote": true,
  "trailingComma": "all",
  "tabWidth": 2,
  "semi": true,
  "printWidth": 80,
  "overrides": [
    {
      "files": ["*.html", "*.py"],
      "options": {
        "tabWidth": 4,
        "semi": false,
        "trailingComma": "none"
      }
    }
  ]
}
